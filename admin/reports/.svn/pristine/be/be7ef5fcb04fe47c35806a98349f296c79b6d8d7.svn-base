<div [@routerTransition]>
    <div class="m-subheader">
        <div class="row align-items-center">
            <div class="mr-auto col-auto">
                <h3 class="m-subheader__title m-subheader__title--separator">
                    <!-- Begin Title -->
                    <span>{{l("ReStatusNotUse")}}</span>
                    <!-- End Title -->
                </h3>
            </div>

        </div>
    </div>

    <div id="content" class="m-content">
        <div class="m-portlet m-portlet--mobile">
            <div class="m-portlet__body">
                <form #editForm="ngForm" novalidate class="horizontal-form filter-form" autocomplete="off">
                    <div class="m-form m-form--label-align-right">
                        <div class="row align-items-center m--margin-bottom-30">
                            <!-- Begin Filter -->

                            <!-- Đơn vị -->
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>{{l("Branch")}}</label>
                                    <div class="user-group">
                                        <input class="form-control" [disabled]="true"
                                            [ngModelOptions]="{standalone: true}" [ngModel]="filterInput.brancH_NAME">
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-primary"
                                                (click)="branchModal.show()">...</button>
                                        </span>
                                    </div>
                                </div>
                            </div>

                            <!-- Từ ngày - Đến ngày -->
                            <div class="col-md-12">
                                <fieldset>
                                    <legend>{{l("InputDate")}}</legend>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <label>{{l("FromDate")}}</label>
                                            <date-control [inpCss]="'form-control'" type="text" [ngModelOptions]="{standalone: true}" [(ngModel)]="filterInput.fromDate">
                                            </date-control>
                                        </div>
                                        <div class="col-md-6 required">
                                            <label>{{l("ToDate")}}</label>
                                            <date-control required [inpCss]="'form-control'" type="text" name="TODATE" #TODATE="ngModel" [(ngModel)]="filterInput.toDate">
                                            </date-control>
                                            <div class="error-message" *ngIf="TODATE.invalid && isShowError">
                                                <span *ngIf="TODATE?.errors.required">{{l('ToDate')}}
                                                        {{l("ValidationRequired")}}</span>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>

                            <!-- Cấp độ -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>{{l("Level")}}</label>
                                    <select2-custom [(ngModel)]="filterInput.level"
                                        [ngModelOptions]="{standalone: true}" [(list)]="levels" [valueMember]="'value'"
                                        [displayMember]="'display'">
                                    </select2-custom>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label></label>
                                    <div>
                                        <!-- <button class="btn btn-primary">{{l('Report')}}</button> -->
                                        <button (click)="exportToExcel()"
                                            class="btn btn-primary">{{l('Excel')}}</button>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<branch-modal (onSelect)="onSelectBranch($event)" [multiple]="false" #branchModal></branch-modal>