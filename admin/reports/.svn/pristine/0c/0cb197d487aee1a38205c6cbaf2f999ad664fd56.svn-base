<div [@routerTransition]>
    <div class="m-subheader">
        <div class="row align-items-center">
            <div class="mr-auto col-auto">
                <h3 class="m-subheader__title m-subheader__title--separator">
                    <!-- Begin Title -->
                    <span>{{l("CARBC09")}}</span>
                    <!-- End Title -->
                </h3>
            </div>

        </div>
    </div>

    <div id="content" class="m-content">
        <div class="m-portlet m-portlet--mobile">
            <div class="m-portlet__body">
                <form class="horizontal-form filter-form" autocomplete="off">
                    <div class="m-form m-form--label-align-right">
                        <div class="row align-items-center m--margin-bottom-30">
                            <!-- Begin Filter -->
                            <!-- Đơn vị -->
                            <div class="col-md-8">
                                <div class="form-group">
                                    <label>{{l("Branch")}}</label>
                                    <div class="input-group">
                                        <input class="form-control" [disabled]="true" [ngModelOptions]="{standalone: true}" [ngModel]="filterInput.brancH_NAME">
                                        <span class="input-group-btn">
                                            <button type="button" class="btn btn-primary"
                                                (click)="showBranchModal()">...</button>
                                        </span>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4">

                            </div>
                            <!-- Đến -->
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>{{l("To")}}</label>
                                    <select2-custom [(ngModel)]="filterInput.toMonth" [ngModelOptions]="{standalone: true}" [(list)]="months" [valueMember]="'value'" [displayMember]="'display'">
                                    </select2-custom>

                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label></label>
                                    <select2-custom [(ngModel)]="filterInput.toYear" [ngModelOptions]="{standalone: true}" [(list)]="years" [valueMember]="'value'" [displayMember]="'display'">
                                    </select2-custom>
                                </div>
                            </div>
                            <div class="col-md-4"></div>

                            <!-- Chọn xe -->
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>{{l("ChooseCar")}}</label>
                                    <div class="user-group">
                                        <select2-custom [(ngModel)]="filterInput.caR_ID" [ngModelOptions]="{standalone: true}" [emptyText]="l('SelectAll')" [(list)]="cars" [valueMember]="'value'" [displayMember]="'display'">
                                        </select2-custom><span class="input-group-btn">
                                            <button type="button" class="btn btn-primary"
                                                (click)="showCarModal()">...</button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6"></div>
                            <!-- Cấp độ -->
                            <div class="col-md-4">
                                <div class="form-group">
                                    <label>{{l("Level")}}</label>
                                    <select2-custom [(ngModel)]="filterInput.level" [ngModelOptions]="{standalone: true}" [(list)]="levels" [valueMember]="'value'" [displayMember]="'display'">
                                    </select2-custom>
                                </div>
                            </div>
                            <!-- Report -->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label></label>
                                    <div>
                                        <button (click)="printCarBC09_p()" class="btn btn-primary">Report</button>
                                        <button (click)="exportToExcel()" class="btn btn-primary">Export Excel</button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <branch-modal [showColAuthStatus]="false" [showColPotential]="false" (onSelect)="onSelectBranch($event)" [multiple]="false" #branchModal></branch-modal>
    <car-master-modal (onSelect)="onSelectCar($event)" [multiple]="false" #carModal></car-master-modal>
</div>